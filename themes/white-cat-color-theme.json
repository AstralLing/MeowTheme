/* 
 * Meow Color Theme @ AstralLing
 * Thanks to Snazzy Light Theme for great color scheme for reference!
 * 
 * Black foreground: #233333
 * White background: #ffffff
 * Main border: #e7e7e7
 * Grey: #ffffff -> #fdfeff -> #fafbfc -> #e7e7e7 -> #c0c0c0 -> #999999
 * 
 * Light blue: #5bcefa, Light pink: #f5a9b8 @https://commons.wikimedia.org/wiki/File:Transgender_Pride_flag.svg
 * Because these two colors are not suitable to be read as text, this theme does not fully adopt them.
 */
{
  "name": "Meow Theme",
  "semanticHighlighting": true,
  "colors": {
    "focusBorder": "#6596ff",
    "foreground": "#233333",
    "editor.foreground": "#233333",
    "editor.background": "#ffffff",
    "editor.lineHighlightBackground": "#5bcefa15",
    "editor.selectionBackground": "#288cff25",
    "editor.inactiveSelectionBackground": "#288cff10",
    "editor.selectionHighlightBackground": "#33cc5530",
    "editor.selectionHighlightBorder": "#33cc5500", // Remove border
    "editorBracketMatch.background": "#33cc5530",
    "editorBracketMatch.border": "#33cc5500",
    "editorLineNumber.foreground": "#c0c0c0",
    "editorLineNumber.activeForeground": "#233333",
    "editorGroupHeader.tabsBackground": "#fafbfc",
    "editorGroupHeader.tabsBorder": "#e7e7e7",
    "editorGroup.border": "#e7e7e7",
    "editorCursor.foreground": "#999999",
    "editor.findMatchBackground": "#ffea95",
    "editor.findMatchHighlightBackground": "#ffea9566",
    "editorWidget.background": "#ffffff",
    "diffEditor.insertedTextBackground": "#42e66830",
    "diffEditor.removedTextBackground": "#dd213430",
    "tab.activeForeground": "#233333",
    "tab.inactiveForeground": "#999999",
    "tab.inactiveBackground": "#fafbfc",
    "tab.activeBackground": "#ffffff",
    "tab.hoverBackground": "#ffffff",
    "tab.unfocusedHoverBackground": "#ffffff",
    "tab.border": "#e7e7e7",
    "titleBar.activeForeground": "#233333",
    "titleBar.activeBackground": "#ffffff",
    "titleBar.inactiveForeground": "#999999",
    "titleBar.inactiveBackground": "#fafbfc",
    "titleBar.border": "#e7e7e7",
    "tab.activeBorder": "#ffffff",
    "tab.unfocusedActiveBorder": "#ffffff",
    "tab.activeBorderTop": "#6596ff",
    "sideBar.foreground": "#233333",
    "sideBar.background": "#fafbfc",
    "sideBar.border": "#e7e7e7",
    "sideBarSectionHeader.foreground": "#233333",
    "sideBarSectionHeader.background": "#fafbfc",
    "sideBarSectionHeader.border": "#e7e7e7",
    "panel.background": "#fafbfc",
    "panel.border": "#e7e7e7",
    "activityBar.foreground": "#233333",
    "activityBar.background": "#ffffff",
    "activityBar.inactiveForeground": "#999999",
    "activityBar.border": "#e7e7e7",
    "activityBar.activeBorder": "#6596ff",
    "activityBarBadge.foreground": "#ffffff",
    "activityBarBadge.background": "#6596ff",
    "statusBar.foreground": "#233333",
    "statusBar.background": "#ffffff",
    "statusBar.border": "#e7e7e7",
    "statusBar.noFolderBackground": "#ffffff",
    "statusBar.debuggingBackground": "#f5a9b8",
    "statusBar.debuggingForeground": "#ffffff",
    "statusBarItem.prominentBackground": "#e7e7e7",
    "button.foreground": "#ffffff",
    "button.background": "#f5a9b8",
    "button.hoverBackground": "#f58ea2",
    "checkbox.background": "#ffffff",
    "checkbox.border": "#e7e7e7",
    "dropdown.background": "#fdfeff",
    "dropdown.border": "#e7e7e7",
    "dropdown.foreground": "#233333",
    "dropdown.listBackground": "#ffffff",
    "list.hoverForeground": "#233333",
    "list.activeSelectionForeground": "#233333",
    "list.activeSelectionBackground": "#e7e7e7",
    "list.inactiveSelectionForeground": "#233333",
    "list.inactiveSelectionBackground": "#e7e7e7",
    "input.background": "#fdfeff",
    "input.border": "#e7e7e7",
    "input.foreground": "#233333",
    "quickInput.background": "#fdfeff",
    "quickInput.foreground": "#233333",
    "textLink.foreground": "#6596ff",
    "textBlockQuote.background": "#fdfeff",
    "editorIndentGuide.background": "#e7e7e7",
    "tree.indentGuidesStroke": "#e7e7e7",
  },
  "tokenColors": [
    // Adapted from Snazzy Light, to be recoded.
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#c0c0c0"
      }
    },
    {
      "name": "Comments: Preprocessor",
      "scope": "comment.block.preprocessor",
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#ebb304"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#adb1c2"
      }
    },
    {
      "name": "Sharp Keywords",
      "scope": [
        "punctuation.definition.directive",
        "keyword.operator.new,keyword.other.new",
        "storage.modifier.c",
        "keyword.control.directive.define",
        "keyword.control.def",
        "keyword.control.class",
        "keyword.control.module",
        "keyword.control.new,keyword.preprocessor.define",
        "meta.preprocessor.cs punctuation.separator.hash.cs",
        "keyword.control.directive.undef",
        "keyword.other.fn",
        "storage.type.rust",
        "keyword.operator.arrow.skinny",
        "keyword.operator.expression.import",
        "keyword.operator.expression.delete",
        "storage.modifier.extern",
        "storage.modifier.const",
        "keyword.control.directive.include",
        "storage.modifier.extends",
        "storage.modifier.ts",
        "keyword.control.at-rule",
        "keyword.other.let",
        "keyword.other.instance",
        "keyword.other.newtype",
        "keyword.other.data",
        "keyword.other.module",
        "keyword.other",
        "keyword.other.preprocessor",
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#7C4DFF"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage",
        "meta.class storage.type"
      ],
      "settings": {
        "foreground": "#ee89d6"
      }
    },
    {
      "name": "Units",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#ebb304",
      },
    },
    {
      "name": "Types",
      "scope": [
        "support.type",
        "meta.type.annotation entity.name.type",
        "new.expr meta.type.parameters entity.name.type",
        "storage.type.primitive",
        "storage.type.built-in.primitive",
        "meta.function.parameter storage.type",
      ],
      "settings": {
        "foreground": "#a356bb"
      }
    },
    {
      "name": "Language Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#a356bb"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#565869"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#6596ff"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "foreground": "#a356bb"
      }
    },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#ebb304"
      }
    },
    {
      "name": "Sections",
      "scope": "entity.name.section",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant"
      ],
      "settings": {
        "foreground": "#ebb304"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#53a053"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#ebb304"
      }
    },
    {
      "name": "Strings: Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#00bec4"
      }
    },
    {
      "name": "Strings: Symbols",
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#53a053"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#adb1c2"
      }
    },
    // Markup Overrides
    {
      "name": "Markup: Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#888888",
      },
    },
    {
      "name": "Markup: Deletion",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#888888",
      },
    },
    {
      "name": "Markup: Emphasis",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
      },
    },
    {
      "name": "Markup: Error",
      "scope": "markup.error",
      "settings": {
        "foreground": "#EBB304",
      },
    },
    {
      "name": "Markup: Insertion",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#888888",
      },
    },
    {
      "name": "Markup: Link",
      "scope": "meta.link",
      "settings": {
        "foreground": "#53A053",
      },
    },
    {
      "name": "Markup: Link Title",
      "scope": "string.other.link.title.markdown",
      "settings": {
        "foreground": "#437AED",
      },
    },
    {
      "name": "Markup: Output",
      "scope": [
        "markup.output",
        "markup.raw",
      ],
      "settings": {
        "foreground": "#999999",
      },
    },
    {
      "name": "Markup: Prompt",
      "scope": "markup.prompt",
      "settings": {
        "foreground": "#999999",
      },
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#A356BB",
      },
    },
    {
      "name": "Markup: Strong",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
      },
    },
    {
      "name": "Markup: Traceback",
      "scope": "markup.traceback",
      "settings": {
        "foreground": "#EBB304",
      },
    },
    {
      "name": "Markup: Underline",
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline",
      },
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#777985",
      },
    },
    {
      "name": "Markup Styling",
      "scope": [
        "markup.bold",
        "markup.italic",
      ],
      "settings": {
        "foreground": "#00BEC4",
      },
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#EE89D6",
      },
    },
    // Makefile Overrides
    {
      "name": "Make: Variables",
      "scope": "source.makefile variable",
      "settings": {
        "foreground": "#11658F",
      },
    },
    // Diff Overrides
    {
      "name": "Diff: Headers",
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#11658F"
      }
    },
    {
      "name": "Diff: Inserted",
      "scope": "markup.inserted.diff",
      "settings": {
        "foreground": "#A356BB"
      }
    },
    {
      "name": "Diff: Deleted",
      "scope": "markup.deleted.diff",
      "settings": {
        "foreground": "#EBB304"
      }
    },
    {
      "name": "Diff: Range",
      "scope": [
        "meta.diff.range",
        "meta.diff.index",
        "meta.separator"
      ],
      "settings": {
        "foreground": "#437AED"
      }
    },
    //--- Other languages ---//
    // JavaScript Overrides
    {
      "name": "JavaScript: Braces and fat arrow",
      "scope": [
        "meta.brace.round",
        "meta.brace.square",
        "storage.type.function.arrow",
      ],
      "settings": {
        "foreground": "#ADB1C2",
      },
    },
    {
      "name": "JavaScript: Special import/export entities",
      "scope": [
        "constant.language.import-export-all",
        "meta.import keyword.control.default"
      ],
      "settings": {
        "foreground": "#C25193",
      },
    },
    {
      "name": "JavaScript: Built-in functions",
      "scope": [
        "support.function.js",
      ],
      "settings": {
        "foreground": "#11658F",
      },
    },
    {
      "name": "JavaScript: RegExp",
      "scope": "string.regexp.js",
      "settings": {
        "foreground": "#00BEC4",
      },
    },
    {
      "name": "JavaScript: super() call and Node.js 'module'",
      "scope": [
        "variable.language.super",
        "support.type.object.module.js",
      ],
      "settings": {
        "foreground": "#EE89D6",
      },
    },
    // HTML Overrides
    {
      "name": "HTML: Doctype Declaration",
      "scope": [
        "meta.tag.metadata.doctype.html entity.name.tag",
        "meta.tag.metadata.doctype.html entity.other.attribute-name.html",
        "meta.tag.sgml.doctype",
        "meta.tag.sgml.doctype string",
        "meta.tag.sgml.doctype entity.name.tag",
        "meta.tag.sgml punctuation.definition.tag.html",
