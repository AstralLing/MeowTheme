        "source.css.scss meta.at-rule variable.parameter",
      ],
      "settings": {
        "foreground": "#ADB1C2",
      },
    },
    // YAML Overrides
    {
      "name": "YAML: Keys",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#11658F",
      },
    },
    {
      "name": "YAML: Keys",
      "scope": "variable.other.alias.yaml",
      "settings": {
